user. Dispatch errors on failure.\nexport const login = user => dispatch => (\n  APIUtil.login(user)\n    .then( res => {\n      const {token} = res.data;\n      localStorage.setItem('jwtToken', token);\n      APIUtil.setAuthToken(token);\n      const decoded = jwt_decode(token);\n      dispatch(receiveCurrentUser(decoded));\n    })\n    .catch(err => {\n      dispatch(receiveErrors(err.response.data));\n    })\n)\n\nexport const logout = () => dispatch => {\n  localStorage.removeItem('jwtToken')\n  APIUtil.setAuthToken(false)\n  dispatch(logoutUser())\n};","/home/rizzacarla/App_Academy_Projects/MERN/Melting-Pot/frontend/src/store/store.js",[],"/home/rizzacarla/App_Academy_Projects/MERN/Melting-Pot/frontend/src/util/session_api_util.js",[],"/home/rizzacarla/App_Academy_Projects/MERN/Melting-Pot/frontend/src/components/root.js",[],"/home/rizzacarla/App_Academy_Projects/MERN/Melting-Pot/frontend/src/components/app.js",[],"/home/rizzacarla/App_Academy_Projects/MERN/Melting-Pot/frontend/src/reducers/root_reducer.js",[],"/home/rizzacarla/App_Academy_Projects/MERN/Melting-Pot/frontend/src/util/route_util.js",[],"/home/rizzacarla/App_Academy_Projects/MERN/Melting-Pot/frontend/src/reducers/session_reducer.js",[],"/home/rizzacarla/App_Academy_Projects/MERN/Melting-Pot/frontend/src/reducers/errors_reducer.js",[],"/home/rizzacarla/App_Academy_Projects/MERN/Melting-Pot/frontend/src/components/nav/navbar_container.js",[],"/home/rizzacarla/App_Academy_Projects/MERN/Melting-Pot/frontend/src/components/session/signup_form_container.js",[],"/home/rizzacarla/App_Academy_Projects/MERN/Melting-Pot/frontend/src/components/session/login_form_container.js",[],"/home/rizzacarla/App_Academy_Projects/MERN/Melting-Pot/frontend/src/components/home/home_page.jsx",[],"/home/rizzacarla/App_Academy_Projects/MERN/Melting-Pot/frontend/src/reducers/session_errors_reducer.js",[],"/home/rizzacarla/App_Academy_Projects/MERN/Melting-Pot/frontend/src/components/nav/navbar.jsx",[],"/home/rizzacarla/App_Aca