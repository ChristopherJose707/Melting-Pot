{handleChange} >\n                    <Tab label=\"My Recipes\"/>\n                    <Tab label=\"Liked Recipes\"/>\n                    <Tab label=\"Events\" />\n                    <Tab label=\"Friends\" />\n                    <Tab label=\"About\" />\n                </Tabs>\n\n                {selectedTab === 0 && <RecipesContainer />}\n                {selectedTab === 1 && <LikesContainer />}\n                {selectedTab === 2 && <EventsContainer />}\n                {selectedTab === 3 && <FriendsContainer />}\n                {selectedTab === 4 && <AboutMeContainer />}\n            </>\n\n\n        )\n    \n}\n\nexport default NavBar;","/Users/kevin.park02/Desktop/Bootcamp/mern_projects/Melting-Pot/frontend/src/components/profile/3.recipes_container.js",[],"/Users/kevin.park02/Desktop/Bootcamp/mern_projects/Melting-Pot/frontend/src/components/profile/8..aboutMe_container.js",[],"/Users/kevin.park02/Desktop/Bootcamp/mern_projects/Melting-Pot/frontend/src/components/profile/4.likes_container.js",[],"/Users/kevin.park02/Desktop/Bootcamp/mern_projects/Melting-Pot/frontend/src/components/profile/5.events_container.js",[],"/Users/kevin.park02/Desktop/Bootcamp/mern_projects/Melting-Pot/frontend/src/components/profile/3.recipes.js",["295"],"import React from 'react'\n\nclass Recipes extends React.Component {\n    constructor(props) {\n        super(props);\n\n    }\n\n    componentDidMount(){\n        this.props.getUserRecipes(this.props.currentUser._id)\n    }\n\n    render() {\n        return (\n            <div>\n                {this.props.recipes.name}\n            </div>\n        )\n    }\n}\n\nexport default Recipes;","/Users/kevin.park02/Desktop/Bootcamp/mern_projects/Melting-Pot/frontend/src/components/profile/7.aboutMe.js",[],"/Users/kevin.park02/Desktop/Bootcamp/mern_projects/Melting-Pot/frontend/src/components/profile/6.friends_container.js",[],"/Users/kevin.park02/Desktop/Bootcamp/mern_projects/Melting-Pot/frontend/src/components/profile/4.likes.js",["296"],"import React from 'react'\n\nclass Likes extends React.Component {\n    constructor(props) {\n        super(props);\n\n    }\n\n    render() {\n        return (\n            <div>\n                Likes component\n            </div>\n        )\n    }\n}\n\nexport default Likes;","/Users/kevin.park02/Desktop/Bootcamp/mern_projects/Melting-Pot/frontend/src/components/profile/5.events.js",["297"],"import React from 'react'\n\nclass Events extends React.Component {\n    constructor(props) {\n        super(props);\n\n    }\n\n    render() {\n        return (\n            <div>\n                Events Component\n            </div>\n        )\n    }\n}\n\nexport default Events;","/Users/kevin.park02/Desktop/Bootcamp/mern_projects/Melting-Pot/frontend/src/components/profile/6.friends.js",["298"],"import React from 'react'\n\nclass Friends extends React.Component {\n    constructor(props) {\n        super(props);\n\n    }\n\n    render() {\n        return (\n            <div>\n                Friend Component\n                <br></br>\n                Friends:\n                {this.props.currentUser.friends.map((friend, i) => (\n                    <div key={i}>\n                        {friend}\n                    </div>\n                ))}\n            </div>\n        )\n    }\n}\n\nexport default Friends;","/Users/kevin.park02/Desktop/Bootcamp/mern_projects/Melting-Pot/frontend/src/util/photo_api_util.js",[],"/Users/kevin.park02/Desktop/Bootcamp/mern_projects/Melting-Pot/frontend/src/actions/photo_actions.js",[],"/Users/kevin.park02/Desktop/Bootcamp/mern_projects/Melting-Pot/frontend/src/reducers/photo_reducer.js",[],"/Users/kevin.park02/Desktop/Bootcamp/mern_projects/Melting-Pot/frontend/src/components/event/1.event_create_container.jsx",[],"/Users/kevin.park02/Desktop/Bootcamp/mern_projects/Melting-Pot/frontend/src/actions/event_actions.js",[],"/Users/kevin.park02/Desktop/Bootcamp/mern_projects/Melting-Pot/frontend/src/components/event/1.event_create.jsx",[],"/Users/kevin.park02/Desktop/Bootcamp/mern_projects/Melting-Pot/frontend/src/components/event/2.event_show_container.jsx",[],"/Users/kevin.park02/Desktop/Bootcamp/mern_projects/Melting-Pot/frontend/src/components/event/2.event_show.jsx",["299"],"import React from 'react';\n\n\nclass EventShow extends React.Component {\n    constructor(props) {\n        super(props)\n    }\n\n    componentDidMount() {\n        this.props.getEvents();\n    }\n\n    render() {\n    \n        return (\n            <div>\n                \n            </div>\n        )\n    }\n}\n\nexport default EventShow;","/Users/kevin.park02/Desktop/Bootcamp/mern_projects/Melting-Pot/frontend/src/components/recipe/recipe_edit.jsx",["300","301","302","303","304","305","306","307","308","309","310"],"import React from 'react';\nimport './recipe_edit.css';\n\nclass RecipeEdit extends React.Component {\n    constructor(props){\n        super(props)\n        this.state = this.props.recipe;\n        this.update = this.update.bind(this);\n        this.handleInstruction = this.handleInstruction.bind(this);\n        this.handleInstClick = this.handleInstClick.bind(this);\n        this.handleIngClick = this.handleIngClick.bind(this);\n        this.handleIngredient = this.handleIngredient.bind(this);\n        this.queueIngredient = this.queueIngredient.bind(this);\n        this.pushIngredient = this.pushIngredient.bind(this);\n        this.queueInstruction = this.queueInstruction.bind(this);\n        this.pushInstruction = this.pushInstruction.bind(this);\n        this.handleDelete = this.handleDelete.bind(this);\n        this.handleSubmit = this.handleSubmit.bind(this);\n    }\n\n    componentDidMount(){\n        this.props.getRecipes();\n        this.props.getUserRecipes(this.props.currentUser._id);\n    }\n    \n    componentDidUpdate(){\n        if(this.state === null){\n            this.setState(Object.assign(this.props.recipe, {ingIdx: \"\", instIdx: \"\", queueIng:\"\", queueInst:\"\" }));\n        }\n    }\n\n    update(field) {\n        return e => this.setState({[field]: e.target.value})\n    }\n\n    handleInstClick(idx) {\n        this.setState({[\"instIdx\"]: idx});\n    }\n\n    handleInstruction(e) {\n        e.preventDefault();\n        let newInstruction = this.state.instructions;\n        newInstruction[this.state.instIdx] = e.target.value;\n        this.setState({[\"instructions\"]: newInstruction});\n    }\n\n    handleIngClick(idx) {\n        this.setState({[\"ingIdx\"]: idx });\n    }\n\n    handleIngredient(e) {\n        e.preventDefault();\n        let newIngredient = this.state.ingredients;\n        newIngredient[this.state.ingIdx] = e.target.value;\n        this.setState({[\"ingredients\"]: newIngredient });\n    }\n\n    queueIngredient(e){\n        e.preventDefault();\n        this.setState({[\"queueIng\"]: e.target.value})\n    }\n    \n    pushIngredient(e) {\n        e.preventDefault();\n        let pushIng = this.state.ingredients.slice()\n        pushIng = pushIng.concat(this.state.queueIng);\n        this.setState({[\"ingredients\"]: pushIng }, () => {\n            this.setState({[\"queueIng\"]: \"\" });\n        });\n    }\n\n    queueInstruction(e){\n        e.preventDefault();\n        this.setState({[\"queueInst\"]: e.target.value})\n    }\n    \n    pushInstruction(e) {\n        e.preventDefault();\n        let pushInst = this.state.instructions.slice()\n        pushInst = pushInst.concat(this.state.queueInst);\n        this.setState({[\"instructions\"]: pushInst }, () => {\n            this.setState({[\"queueInst\"]: \"\" });\n        });\n    }\n\n    handleDelete(e) {\n        e.preventDefault();\n        this.props.deleteRecipe(this.props.recipe._id).then(this.props.history.push(`/profile`))\n    }\n\n    handleSubmit(e) {\n        e.preventDefault();\n        this.props.updateRecipe(this.state).then(() => this.props.history.push(`/recipes/${this.props.recipe._id}`));\n    }\n\n    render() {\n        const recipe = this.props.recipe;\n\n        if (this.state === null) {\n            return null\n        }\n        if (this.props.currentUser._id !== recipe.authorId) {\n            this.props.history.push('/')\n        }\n        console.log(this.props.currentUser._id)\n        console.log(recipe.authorId)\n        return (\n          <div className=\"recipe-edit-parent\">\n            <div className=\"recipe-edit\">\n              <div className=\"recipe-edit-top\">\n                <div className=\"recipe-edit-top-left\">\n                  <div className=\"recipe-edit-img-parent\">\n                    <div className=\"recipe-edit-img\">\n                      <img src={recipe.photoUrl}></img>\n                    </div>\n                    <label className=\"recipe-edit-name\">\n                      Recipe Name:\n                      <input\n                        type=\"text\"\n                        value={this.state.name}\n                        placeholder={recipe.name}\n                        onChange={this.update(\"name\")}\n                      />\n                    </label>\n                  </div>\n                </div>\n                <div className=\"recipe-edit-top-right\">\n                  <label>\n                    Difficulty:\n                    <select\n                      onChange={this.update(\"difficulty\")}\n                      value={this.state.difficulty}\n                    >\n                      <option value=\"easy\">Easy</option>\n                      <option value=\"medium\">Medium</option>\n                      <option value=\"hard\">Hard</option>\n                    </select>\n                  </label>\n                  <label>\n                    Cooking Time:\n                    <select\n                      onChange={this.update(\"cookingTime\")}\n                      value={this.state.cookingTime}\n                    >\n                      <option value=\"0.5 hr\">0.5 hr</option>\n                      <option value=\"1 hr\">1 hr</option>\n                      <option value=\"1.5 hr\">1.5 hr</option>\n                      <option value=\"2+ hrs\">2+ hr</option>\n                    </select>\n                  </label>\n                  <label>\n                    Category:&nbsp;&nbsp;\n                    <select\n                      onChange={this.update(\"category\")}\n                      value={this.state.category}\n                    >\n                      <option value=\"Meat\">Meat</option>\n                      <option value=\"Vegetables\">Vegetables</option>\n                      <option value=\"Poultry\">Poultry</option>\n                      <option value=\"Carbs\">Carbs</option>\n                      <option value=\"Noodle\">Soup</option>\n                    </select>\n                  </label>\n                  <label>\n                    Likes: {!recipe.numLikes ? \"0\" : recipe.numLikes}\n                  </label>\n                  {/* <button onClick={this.handleSubmit}>Save Recipe</button> */}\n                </div>\n              </div>\n              <div className=\"recipe-edit-bottom\">\n                <div className=\"recipe-edit-bottom-left\">\n                  <div className=\"recipe-edit-ingredients\">\n                    <label>Ingredients </label>\n                    <ul>\n                      {this.state.ingredients.map((ingredient, idx) => {\n                        return (\n                          <li key={idx}>\n                            <textarea\n                              className=\"recipe-edit-textarea\"\n                              type=\"text\"\n                              value={this.state.ingredients[idx]}\n                              placeholder={ingredient}\n                              onClick={() => this.handleIngClick(idx)}\n                              onChange={this.handleIngredient}\n                            />\n                          </li>\n                        );\n              