const [selectedTab, setSelectedTab] = React.useState(0);\n\n    const handleChange = (event, newValue) => {\n        setSelectedTab(newValue);\n    };\n\n        return (\n            <>\n                <Tabs value={selectedTab} onChange={handleChange} >\n                    <Tab label=\"My Recipes\"/>\n                    <Tab label=\"Liked Recipes\"/>\n                    <Tab label=\"Events\" />\n                    <Tab label=\"Friends\" />\n                    <Tab label=\"About\" />\n                </Tabs>\n\n                {selectedTab === 0 && <RecipesContainer />}\n                {selectedTab === 1 && <LikesContainer />}\n                {selectedTab === 2 && <EventsContainer />}\n                {selectedTab === 3 && <FriendsContainer />}\n                {selectedTab === 4 && <AboutMeContainer />}\n            </>\n\n\n        )\n    \n}\n\nexport default NavBar;","/Users/kevin.park02/Desktop/Bootcamp/mern_projects/Melting-Pot/frontend/src/components/profile/3.recipes_container.js",[],"/Users/kevin.park02/Desktop/Bootcamp/mern_projects/Melting-Pot/frontend/src/components/profile/8..aboutMe_container.js",[],"/Users/kevin.park02/Desktop/Bootcamp/mern_projects/Melting-Pot/frontend/src/components/profile/4.likes_container.js",[],"/Users/kevin.park02/Desktop/Bootcamp/mern_projects/Melting-Pot/frontend/src/components/profile/5.events_container.js",[],"/Users/kevin.park02/Desktop/Bootcamp/mern_projects/Melting-Pot/frontend/src/components/profile/3.recipes.js",["294"],"import React from 'react'\n\nclass Recipes extends React.Component {\n    constructor(props) {\n        super(props);\n\n    }\n\n    componentDidMount(){\n        this.props.getUserRecipes(this.props.currentUser._id)\n    }\n\n    render() {\n        return (\n            <div>\n                {this.props.recipes.name}\n            </div>\n        )\n    }\n}\n\nexport default Recipes;","/Users/kevin.park02/Desktop/Bootcamp/mern_projects/Melting-Pot/frontend/src/components/profile/7.aboutMe.j